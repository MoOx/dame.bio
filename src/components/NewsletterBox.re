let styles =
  Style.(
    StyleSheet.create({
      "container":
        style(
          ~paddingTop=30.->dp,
          // ~paddingHorizontal=10.->dp,
          // ~paddingBottom=20.->dp,
          ~maxWidth=400.->dp,
          ~width=100.->pct,
          // ~alignItems=`center,
          ~borderRadius=Consts.Radius.box,
          // for radius/image inside
          ~overflow=`hidden,
          ~backgroundColor=Consts.Colors.lightest,
          ~shadowColor="#333333",
          ~shadowOffset=offset(~height=4., ~width=0.),
          ~shadowOpacity=0.05,
          ~shadowRadius=20.,
          (),
        ),
      "topLeft": style(~position=`absolute, ~top=0.->dp, ~left=0.->dp, ()),
      "bottomRight":
        style(~position=`absolute, ~bottom=0.->dp, ~right=0.->dp, ()),
      "bottom": style(~position=`absolute, ~bottom=0.->dp, ()),
      "bottomLeft":
        style(~position=`absolute, ~bottom=0.->dp, ~left=0.->dp, ()),
      "topRight":
        viewStyle(~position=`absolute, ~top=0.->dp, ~right=0.->dp, ()),
      "title":
        textStyle(
          ~textAlign=`center,
          ~fontSize=22.,
          ~color=Consts.Colors.pink,
          (),
        ),
      "labelWrapper":
        viewStyle(~paddingHorizontal=(Spacer.space /. 2.)->dp, ()),
      "label":
        textStyle(
          ~color=Consts.Colors.darkLabel,
          ~fontWeight=`_600,
          ~lineHeight=24.,
          ~textDecorationLine=`none,
          (),
        ),
      "labelDetail":
        textStyle(~fontSize=11., ~lineHeight=24., ~color="#bdbdbd", ()),
      "field":
        textStyle(
          ~color=Consts.Colors.darkest,
          ~borderWidth=1.,
          ~borderColor=Consts.Colors.pink,
          ~padding=(Spacer.space /. 2.)->dp,
          ~borderRadius=Consts.Radius.field,
          (),
        ),
      "choice":
        viewStyle(
          ~justifyContent=`spaceBetween,
          ~flexDirection=`row,
          ~paddingHorizontal=(Spacer.space /. 2.)->dp,
          (),
        ),
      "success":
        textStyle(
          ~color=Consts.Colors.success,
          ~fontWeight=`_600,
          ~lineHeight=24.,
          ~textDecorationLine=`none,
          ~paddingHorizontal=(Spacer.space /. 2.)->dp,
          (),
        ),
      "error":
        textStyle(
          ~color=Consts.Colors.error,
          ~fontWeight=`_600,
          ~lineHeight=24.,
          ~textDecorationLine=`none,
          ~paddingHorizontal=(Spacer.space /. 2.)->dp,
          (),
        ),
      "button":
        textStyle(
          ~fontSize=20.,
          ~borderRadius=Consts.Radius.field,
          ~paddingVertical=10.->dp,
          ~paddingHorizontal=20.->dp,
          ~justifyContent=`center,
          ~alignItems=`center,
          ~color="#fff",
          ~backgroundColor=Consts.Colors.pink,
          ~opacity=0.75,
          (),
        ),
      "buttonText": textStyle(~color=Consts.Colors.lightest, ()),
    })
  );
module Subscribe = [%graphql
  {|
  mutation subscribe(
    $email: String!,
    $firstname: String,
    $lastname: String,
    $lists: [Int!]!
  ) {
    mailpoetSubscribe(input: {
      clientMutationId: "NewsletterBox",
      email: $email,
      firstname: $firstname,
      lastname: $lastname, lists: $lists}
    ) {
      error
    }
  }
|}
];

[@react.component]
let make = () => {
  let (interactedWith, setInteractedWith) = React.useState(() => false);
  let (subscribed, setSubscribed) = React.useState(() => false);
  let (error, setError) = React.useState(() => None);
  let (email, setEmail) = React.useState(() => Some(User.getEmail()));
  let (lastname, setLastname) = React.useState(() => None);
  let (firstname, setFirstname) = React.useState(() => Some(User.getName()));
  let (listPerPost, setListPerPost) = React.useState(() => Some(true));
  let (listPerMonth, setListPerMonth) = React.useState(() => None);
  let (newsletter, newsletter_result) = Subscribe.use();
  <SpacedView style=styles##container horizontal=XL>
    <View pointerEvents=`none style=styles##topLeft>
      <Image resizeMode=`contain source=topLeft />
    </View>
    <View pointerEvents=`none style=styles##topRight>
      <Image resizeMode=`contain source=topRightFlowers />
    </View>
    <View pointerEvents=`none style=styles##bottomLeft>
      <Image resizeMode=`contain source=bottomLeftFlowers />
    </View>
    <View pointerEvents=`none style=styles##bottomRight>
      <Image resizeMode=`contain source=bottomRight />
    </View>
    <View pointerEvents=`none style=styles##bottom>
      <Image resizeMode=`contain source=bottom />
    </View>
    <Text style=styles##title> "Newsletter"->React.string </Text>
    <Spacer />
    <noscript>
      <Text>
        {j|🚨 Veuillez |j}->React.string
        <a target="_blank" href="https://www.enable-javascript.com/fr/">
          {j|activer JavaScript|j}->React.string
        </a>
        {j| pour vous inscrire à la newsletter.|j}->React.string
      </Text>
      <Spacer />
    </noscript>
    <Text style=styles##labelWrapper>
      <Text style=styles##label> {j|Email *|j}->React.string </Text>
    </Text>
    <TextInput
      style=styles##field
      value={email->Option.getWithDefault("")}
      onChangeText={text => {
        setEmail(_ => Some(text));
        setError(_ => None);
      }}
      onFocus={_ => setInteractedWith(_ => true)}
      autoCapitalize=`none
      autoCorrect=false
      clearButtonMode=`always
      keyboardType=`emailAddress
      textContentType=`emailAddress
      placeholder={j|e@mail *|j}
    />
    <Spacer size=XXS />
    {!interactedWith
       ? <TouchableOpacity
           style=styles##button onPress={_ => setInteractedWith(_ => true)}>
           <Text style=styles##buttonText>
             {j|Je m'abonne|j}->React.string
           </Text>
         </TouchableOpacity>
       : <>
           <Text style=styles##labelWrapper>
             <Text style=styles##label>
               {j|Prénom ou Pseudo|j}->React.string
             </Text>
             <Spacer size=XXS />
             <Text style=styles##labelDetail>
               {j|(optionel)|j}->React.string
             </Text>
           </Text>
           <TextInput
             style=styles##field
             defaultValue={firstname->Option.getWithDefault("")}
             onChangeText={text => {
               setFirstname(_ => Some(text));
               setError(_ => None);
             }}
           />
           <Spacer size=XXS />
           <Text style=styles##labelWrapper>
             <Text style=styles##label> {j|Nom|j}->React.string </Text>
             <Spacer size=XXS />
             <Text style=styles##labelDetail>
               {j|(optionel)|j}->React.string
             </Text>
           </Text>
           <TextInput
             style=styles##field
             defaultValue={lastname->Option.getWithDefault("")}
             onChangeText={text => {
               setLastname(_ => Some(text));
               setError(_ => None);
             }}
           />
           <Spacer size=XS />
           <View style=styles##choice>
             <Text style=styles##label>
               {j|Pour chaque article |j}->React.string
             </Text>
             <Switch
               value={listPerPost->Option.getWithDefault(false)}
               onValueChange={value => setListPerPost(_ => Some(value))}
             />
           </View>
           <View style=styles##choice>
             <Text style=styles##label>
               {j|Une fois par mois |j}->React.string
             </Text>
             <Switch
               value={listPerMonth->Option.getWithDefault(false)}
               onValueChange={value => setListPerMonth(_ => Some(value))}
             />
           </View>
           {error
            ->Option.map(error =>
                <>
                  <Spacer size=XS />
                  <Text style=styles##error> error->React.string </Text>
                </>
              )
            ->Option.getWithDefault(React.null)}
           <Spacer size=XS />
           {!subscribed
              ? if (!newsletter_result.loading) {
                  <TouchableOpacity
                    style=styles##button
                    onPress={_ => {
                      let lists =
                        [||]
                        ->Array.concat(
                            listPerPost
                            ->Option.map(v =>
                                v ? [|mailpoetListIdPerPost|] : [||]
                              )
                            ->Option.getWithDefault([||]),
                          )
                        ->Array.concat(
                            listPerMonth
                            ->Option.map(v =>
                                v ? [|mailpoetListIdPerMonth|] : [||]
                              )
                            ->Option.getWithDefault([||]),
                          );
                      newsletter({
                        email: email->Option.getWithDefault(""),
                        firstname,
                        lastname,
                        lists,
                      })
                      ->FutureJs.fromPromise(Js.String.make)
                      ->Future.tap(
                          fun
                          | Ok(response) => {
                              switch (response) {
                              | Ok({data}) =>
                                data.mailpoetSubscribe
                                ->Option.map(ms =>
                                    ms.error
                                    ->Option.map(error =>
                                        if (error === 0) {
                                          setSubscribed(_ => true);
                                        } else {
                                          let errorMessage =
                                            switch (error) {
                                            // https://github.com/mailpoet/mailpoet/blob/5214e93787db6ccec5ce00becd9349667f861f0c/lib/API/MP/v1/APIException.php
                                            | 3 => {j|Veuillez choisir une liste au minimum|j}
                                            | 10 => {j|Impossible d'envoyer l'email de confirmation|j}
                                            | 11 => {j|L'email est obligatoire|j}
                                            | 12 => {j|Vous êtes déjà inscrit|j}
                                            | 13 => {j|Votre email semble incorrect|j}
                                            | _ =>
                                              {j|Ouuuups, quelque chose d'inattendue est arrivée (code: |j}
                                              ++ error->string_of_int
                                              ++ ")"
                                            };
                                          setError(_ => Some(errorMessage));
                                        }
                                      )
                                    ->ignore
                                  )
                                ->ignore
                              | _ =>
                                setError(_ =>
                                  Some(
                                    {j|Ouuuups, quelque chose d'inattendue est arrivée|j},
                                  )
                                )
                              };
                            }
                          | _ =>
                            setError(_ =>
                              Some(
                                {j|Ouuuups, quelque chose d'inattendue est arrivée|j},
                              )
                            ),
                        )
                      ->ignore;
                    }}>
                    <Text style=styles##buttonText>
                      {j|Je m'abonne|j}->React.string
                    </Text>
                  </TouchableOpacity>;
                } else {
                  <View style=styles##button>
                    <Text style=styles##buttonText>
                      <ActivityIndicator
                        size=ActivityIndicator.Size.small
                        color=Consts.Colors.lightest
                      />
                    </Text>
                  </View>;
                }
              : <>
                  <Spacer />
                  <Text style=styles##success>
                    {j|✓ Une confirmation vous a été envoyé, pensez à valider votre abonnement en cliquant dans le lien situé dans le mail !|j}
                    ->React.string
                  </Text>
                </>}
         </>}
  </SpacedView>;
};
